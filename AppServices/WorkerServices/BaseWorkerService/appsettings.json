{
  "Serilog": {
    "Using": [ "Serilog.Filters.Expressions" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "===> {Timestamp:HH:mm:ss.fff zzz} [{ThreadId}] [{Level}] {Message}{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/Worker-.txt",
          "outputTemplate": "===> {Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level}] {Message}{NewLine}{Exception}",
          "retainedFileCountLimit": 31,
          "rollingInterval": "Day",
          "fileSizeLimitBytes": 104857600,
          "rollOnFileSizeLimit": true
        }
      },
      {
        "Name": "Logger",
        "Args": {
          "configureLogger": {
            "MinimumLevel": "Information",
            "WriteTo": [
              {
                "Name": "File",
                "Args": {
                  "path": "logs/Worker.Error-.txt",
                  "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level}] ({SourceContext}) {Message}{NewLine}{Exception}",
                  "retainedFileCountLimit": 31,
                  "rollingInterval": "Day",
                  "fileSizeLimitBytes": 104857600,
                  "rollOnFileSizeLimit": true
                }
              }
            ]
          }
        }
      }
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "RabbitMQSettings": {    
    "WorkerName": "Base",
    "Queues": [
      {
        "QueuePriority": "low",
        "QueueName": "base.low.data",
        "ExchangeName": "base.low.exchange",
        "RoutingKey": "base.low.key",
        "PrefetchCount": 1,
        "RetryCount": 0,
        "RetryQueues": [
          {
            "RetryNumber": 1,
            "RetryQueueName": "base.low.faulted.data.first",
            "RetryExchangeName": "base.low.faulted.exchange.first",
            "RetryMessageTTL": 3000
          },
          {
            "RetryNumber": 2,
            "RetryQueueName": "base.low.faulted.data.second",
            "RetryExchangeName": "base.low.faulted.exchange.second",
            "RetryMessageTTL": 5000
          },
